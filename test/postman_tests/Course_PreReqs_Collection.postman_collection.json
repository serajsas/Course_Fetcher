	"item": [
		{
			"name": "Test dept and number both exist Okanagan campus",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const responseJson = pm.response.json();",
							"",
							"pm.test(\"Response should return 200\", () => {",
							"    pm.response.to.have.status(200);",
							"})",
							"const course = responseJson.data;",
							"",
							"pm.test(\"Response should contain the department and number for requested course\", () => {",
							"    return (pm.expect(course.courseDepartment).to.be.equal(Object.values(pm.request.toJSON().url.query[0])[1]) && ",
							"    pm.expect(course.courseNumber).to.be.equal(parseInt(Object.values(pm.request.toJSON().url.query[1])[1])))",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://localhost:8000/api/v1/course/?dept=CRWR&number=160",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8000",
					"path": [
						"api",
						"v1",
						"course",
						""
					],
					"query": [
						{
							"key": "dept",
							"value": "CRWR"
						},
						{
							"key": "number",
							"value": "160"
						},
						{
							"key": "campus",
							"value": "UBCO",
							"disabled": true
						}
					]
				}
			},
			"response": []
		},
